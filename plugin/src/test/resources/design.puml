@startuml

skinparam componentStyle uml2
skinparam component {
  BorderColor #a0a0a0
  BackgroundColor #f8f8f8
}

[Blamer] <<..blame..>>
[Miner] <<..miner>>
[Reference] <<..reference>>

[Utilities] <<..util>>

[Blamer] --> [Utilities]
[Miner] --> [Utilities]
[Miner] -> [Reference]
[Reference] --> [Utilities]

@enduml
